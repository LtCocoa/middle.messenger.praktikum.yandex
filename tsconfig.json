{
  "compilerOptions": {
    "outDir": "dist",
    "target": "es5",
    "module": "node",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "sourceMap": true,
    "strictNullChecks": true,
    "allowSyntheticDefaultImports": true
  }
}