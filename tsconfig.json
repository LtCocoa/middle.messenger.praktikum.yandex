{
  "compilerOptions": {
    "outDir": "dist",
    "target": "es2017",
    "module": "ES6",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "sourceMap": true,
    "strictNullChecks": true,
    "allowSyntheticDefaultImports": true
  }
}