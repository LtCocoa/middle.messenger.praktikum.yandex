{
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "max-len": [
      "error",
      100
    ],
    "@typescript-eslint/no-unused-vars": 2,
    "quotes": [
      "error",
      "single",
      {
        "allowTemplateLiterals": true
      }
    ],
    "no-var": [
      "error"
    ],
    "semi": [
      "warn",
      "always"
    ]
  }
}